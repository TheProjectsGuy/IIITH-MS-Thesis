%%% Much of this is from the original make title command from uiucthesis.sty. 
%%% I didn't have the time or inclination to fix it.  It should really
%%% support 2 line titles and stuff.  Feel free to impliment it if you
%%% wish.
%%%
%%% Command to generate title page.
%%%
\newcommand{\thesistitlepage}{
    \begingroup
    \begin{spacing}{1}
    \lineskip=0pt
    \lineskiplimit=\maxdimen
    \thispagestyle{empty}
    %\vspace*{\1.01875in}%
    \vspace*{\@titlevspace}%
    \vtop to 0in{%
    %the ``kern'' in the next line was -.25in. klm
    \vtop to 0in{\begin{center}\kern -.125in
    \begin{tabular}{c}\uppercase\expandafter{\thesistitle}
    \end{tabular}\end{center}%
    \vss}
    %
    % \vtop to 0in{\centerline{\uppercase\expandafter{\@title}}\vss}%
    %
    %  skip to ``by'' and author line; measurements w.r.t top line (2.25in)
    %   
    % The kern in the next line was 1.89 for a single-line title       
    %\kern 1.85in%
    \kern \@titlekern%
    \vtop to 0in{\centerline{BY}%
    \vskip \baselineskip
    \centerline{\uppercase\expandafter{\thesistitlepageauthor}}%       
    \vskip \baselineskip
    \begin{center}\begin{tabular}{c}\previousschools\end{tabular}\end{center}%
    \vss}\kern -1.75in%
    %
    %  Skip down from last item (which has baseline at the top) to     
    %  thesis line.
    %
    % The kern in the next line was 4.4in for a single-line title      
    \kern 4.25in%
    \vtop to 0in{%
    \centerline{THESIS}%
    \vskip \baselineskip
    \centerline{\strut Submitted in partial fulfillment of the
    requirements}% 
    \centerline{\strut for the degree of \thesisdegree \, in \thesisdepartment
     }%
    \centerline{\strut in the Graduate College of the}%
    %\centerline{\strut Department of Electrical and Computer Engineering}
    \centerline{\strut \@universityname, \thesisyear}%
    \vss}\kern -4.25in%
    %
    % The kern in the next line was 6.91in for a single-line title    
    \kern 6.75in%
    \vtop to 0in{\centerline{Urbana, Illinois}\vss}%
    \kern -6.75in\vss}
    \end{spacing}
    \endgroup
    \newpage}

